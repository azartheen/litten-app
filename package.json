{
  "name": "litten",
  "description": "Finding new friends for furry friends",
  "version": "0.0.1",
  "private": true,
  "main": "lib",
  "keywords": [
    "react-native"
  ],
  "author": {
    "name": "Joao Carmo",
    "email": "17721880+joaocarmo@users.noreply.github.com"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/joaocarmo/litten-ios/issues"
  },
  "homepage": "https://github.com/joaocarmo/litten-ios#readme",
  "contributors": [],
  "scripts": {
    "react-devtools": "react-devtools",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --forceExit",
    "lint": "eslint .",
    "start:firebase": "firebase emulators:start",
    "generate-data": "./scripts/generate-data",
    "sort-data": "./scripts/sort-data",
    "prepare-data": "yarn generate-data && yarn sort-data",
    "clean-cache": "watchman watch-del-all && rm -rf node_modules/ && yarn install && rm -rf /tmp/metro-* && yarn start --reset-cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.6.5",
    "@react-native-firebase/app": "^8.0.0",
    "@react-native-firebase/auth": "^8.0.9",
    "@react-native-firebase/firestore": "^7.2.3",
    "@react-native-firebase/storage": "^7.1.7",
    "@react-navigation/bottom-tabs": "^5.6.1",
    "@react-navigation/native": "^5.6.1",
    "@react-navigation/stack": "^5.6.2",
    "i18n-js": "^3.7.1",
    "immer": "^7.0.5",
    "libphonenumber-js": "^1.7.54",
    "lodash.isempty": "^4.4.0",
    "lodash.memoize": "^4.1.2",
    "md5-hex": "^3.0.1",
    "react": "16.13.1",
    "react-native": "0.62.2",
    "react-native-device-info": "^5.6.1",
    "react-native-dotenv": "^0.2.0",
    "react-native-expo-viewport-units": "^0.0.8",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-image-crop-picker": "^0.32.0",
    "react-native-localize": "^1.4.0",
    "react-native-maps": "^0.27.1",
    "react-native-popup-menu": "^0.15.8",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.0.7",
    "react-native-screens": "^2.9.0",
    "react-native-splash-screen": "^3.2.0",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/runtime": "^7.10.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.1.0",
    "eslint": "^7.3.1",
    "eslint-plugin-jest": "^23.17.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-fetch-mock": "^3.0.3",
    "metro-react-native-babel-preset": "^0.60.0",
    "react-devtools": "^4.7.0",
    "react-test-renderer": "16.13.1",
    "remote-redux-devtools": "^0.5.16",
    "world-countries": "^4.0.0"
  },
  "jest": {
    "preset": "react-native",
    "timers": "fake",
    "setupFiles": [
      "<rootDir>/node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@react-native-community/*|react-navigation|react-navigation-stack|@react-navigation/.*)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/lib/__mocks__/file-mock.js"
    }
  }
}
