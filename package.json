  "scripts": {
    "android": "react-native run-android",
    "clear-cache": "watchman watch-del-all && rm -rf node_modules/ && yarn install && rm -rf /tmp/metro-* && yarn start --reset-cache",
    "generate-data": "./scripts/generate-data",
    "ios": "react-native run-ios",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "pods": "npx pod-install",
    "postinstall": "patch-package",
    "prepare-data": "yarn generate-data && yarn sort-data",
    "pretty:fix": "prettier --write .",
    "pretty": "prettier --check .",
    "push": "git add . && gitmoji -c && git push",
    "react-devtools": "react-devtools",
    "sort-data": "./scripts/sort-data",
    "start:firebase": "firebase emulators:start",
    "start": "react-native start",
    "storybook": "(adb reverse tcp:7007 tcp:7007 || true) && start-storybook",
    "test": "jest --forceExit",
    "type-check": "yarn flow"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn pretty:fix"
    }
  },
